{
     "hosting": {
       "public": "dist",
       "ignore": [
         "firebase.json",
         "**/.*",
         "**/node_modules/**"
       ],
       "rewrites": [
         {
           "source": "/**",
           "destination": "/index.html"
         }
       ],
       "redirects": [
         {
           "source": "/cv",
           "destination": "https://drive.google.com/file/d/1ypkmvNxf4ZNEo4kBh01O38uD539V-2ca/view",
           "type": 302
         },
         {
           "source": "/en/cv",
           "destination": "https://drive.google.com/file/d/1W1c5wMeKC0w7r-W-xoKyuJPySc2guPjy/view",
           "type": 302
         },
         {
           "source": "/gh",
           "destination": "https://github.com/Estebanbss",
           "type": 302
         },
         {
           "source": "/git",
           "destination": "https://github.com/Estebanbss/website",
           "type": 302
         },
         {
           "source": "/ln",
           "destination": "https://www.linkedin.com/in/estebanbss/",
           "type": 302
         }
       ],
       "headers": [
         {
           "source": "**/*.@(jpg|jpeg|gif|png|svg)",
           "headers": [
             {
               "key": "Cache-Control",
               "value": "max-age=604800"
             }
           ]
         },
         {
           "source": "**/*.@(css|js)",
           "headers": [
             {
               "key": "Cache-Control",
               "value": "max-age=604800"
             }
           ]
         }
       ]
     }
   }
   